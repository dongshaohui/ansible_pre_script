    <style type="text/css">
        body {
            font-family: SimSun;
            -moz-user-select: none;
        }

        wrap {
            white-space: normal;
            display: inline-block;
            margin-left: auto;
            margin-right: auto
        }

        table {
            border: 1px solid #000000;
            border-collapse: collapse;
            table-layout: fixed;
        }

        td {
            border: 1px solid #000000;
            padding-top: 10px;
            padding-bottom: 10px;
        }

        th {
            border: 1px solid black;
            padding-top: 10px;
            padding-bottom: 10px;
        }

        p {
            margin-top: 6px;
            margin-bottom: 6px;
        }

        .title {
            margin-top: 40px;
            margin-bottom: 40px;
        }

        .row {
            position: relative;
            margin-bottom: 20px;
        }

        .column {
            position: absolute;
            left: 0;
        }

        .paddingLeft {
            padding-left: 72px;
        }

