        }

        .alignCenter {
            text-align: center;
        }

        .alignRight {
            text-align: right;
        }

        .font-bold {
            font-family: SimHei;
        }

        @page {
            margin-bottom: 70px;
            counter-increment: page;

            @bottom-center {
                content: counter(page) " / " counter(pages);
            }
